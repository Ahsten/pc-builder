<!DOCTYPE html>
<html>
    <head>
        <title>Categories</title>
    </head>
    <body>
        <h1>Categories</h1>
        <ul>
            <% categories.map((category)=> { %>
                <li>
                    <a href="<%=category.url%>"><%= category.name %></a>
                </li>
            <% }) %>
        </ul>
    </body>
</html>